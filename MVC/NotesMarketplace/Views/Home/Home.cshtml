<!-- How It work -->
<section id="home-how" class="content-box-md-2">

    <div class="container">
        <div class="row text-center">
            <div class="col-md-12">
                <h2>How it Works</h2>
            </div>
        </div>
        <form method="post">
            <div class="row text-center">
                <div class="col-md-6">
                    <div class="home-box">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="myimg2">
                                    <img src="~/Content/img/Homepage/download.png" alt="Download" class="img-fluid">
                                </div>
                                <h3>Download Free/Paid Notes</h3>
                                <p>Get Material for your<br>Cource etc.</p>
                            </div>

                        </div>
                        <div class="row justify-content-center">
                            <div class="col-md-6 col-sm-6 col-6">
                                <button type="submit" class="btn text-uppercase text-center" id="mybtn" name="Download" value="Download">Download</button>
                            </div>
                        </div>
                    </div>
                </div> 
                <div class="col-md-6">
                    <div class="home-box">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="myimg2">
                                    <img src="~/Content/img/home/seller.png" alt="Download" class="img-fluid">
                                </div>
                                <h3>Seller</h3>
                                <p>Upload and Download Cource<br>and Materials etc.</p>
                            </div>

                        </div>
                        <div class="row justify-content-center">
                            <div class="col-md-6 col-sm-6 col-6">
                                <button type="submit" class="btn text-uppercase text-center" id="mybtn" name="Sell" value="Sell">sell book</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>

</section>
<!--How It Work ENds -->
